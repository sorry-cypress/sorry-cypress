{
  "name": "@sorry-cypress/dashboard",
  "version": "1.0.0-beta",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agoldis/sorry-cypress.git"
  },
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "dev:app": "NODE_ENV=development nodemon --watch ./server ./server/",
    "dev": "run-p dev:app generate:watch",
    "start": "node ./server",
    "build": "npm run generate && npm run build:webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "build:webpack": "webpack --mode production",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.15",
    "bold-ui": "^1.0.0-beta.19",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "lodash.capitalize": "^4.2.1",
    "lodash.truncate": "^4.4.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/introspection": "1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@graphql-codegen/typescript-react-apollo": "^1.8.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-tooltip": "^3.9.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.7.1"
  }
}
