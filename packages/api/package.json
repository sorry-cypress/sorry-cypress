{
  "name": "@sorry-cypress/api",
  "version": "1.0.0-rc.2",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agoldis/sorry-cypress.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/typings",
  "scripts": {
    "dev": "run-p generate:watch nodemon types:watch",
    "nodemon": "nodemon",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "build": "npm run generate && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "types:watch": "tsc --noEmit --watch",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline --copy-files",
    "start": "node ./dist/",
    "clean": "rimraf ./dist",
    "test": "echo \"jest\"",
    "lint": "eslint ./ --ext ts",
    "release": "yarn version --no-git-tag-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.13.4",
    "@graphql-codegen/typescript-resolvers": "^1.13.4",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/lodash": "^4.14.161",
    "@types/mongodb": "^3.5.32",
    "@types/node": "^14.11.2",
    "@types/uuid": "^8.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^6.2.6",
    "@graphql-tools/load": "^6.2.5",
    "@sorry-cypress/common": "1.0.0",
    "apollo-server": "^2.18.1",
    "dotenv": "^8.1.0",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.2",
    "plur": "^4.0.0",
    "pretty-quick": "^2.0.1",
    "uuid": "^3.3.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/?(*.)(spec|test).(t)s?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\](node_modules)[/\\\\].+\\.(js|mjs|ts)$"
    ],
    "moduleNameMapper": {
      "^@src/(.*)": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "d.ts"
    ]
  }
}
