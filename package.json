{
  "name": "sorry-cypress",
  "version": "2.6.0",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agoldis/sorry-cypress.git"
  },
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "@types/express": "4.17.12",
    "undici": "5.5.1",
    "ansi-regex": "5.0.1",
    "xml2js": "0.5.0",
    "json5": "2.2.3",
    "node-forge": "1.3.0",
    "@types/serve-static": "1.13.10",
    "pac-resolver": "^7.0.0",
    "loader-utils": "^2.0.4"
  },
  "scripts": {
    "killall": "ps -ef | egrep \"webpack|node|nodemon\" | grep sorry | awk '{ print $2 }' | xargs kill",
    "release": "./scripts/release-github.sh",
    "release-packages": "wsrun run release --cache-dir=.turbo",
    "build": "turbo run build --cache-dir=.turbo",
    "generate": "yarn --cwd packages/api generate && yarn --cwd packages/dashboard generate",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev": "APOLLO_PLAYGROUND=true turbo run dev",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-react": "^7.21.2",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "pretty-quick": "^3.0.2",
    "rimraf": "^3.0.2",
    "semver": "^7.5.2",
    "tsconfig-paths": "^4.1.0",
    "turbo": "^1.9.1",
    "typescript": "^4.0.3",
    "wsrun": "^5.2.4"
  },
  "packageManager": "yarn@1.22.19"
}
